"udp|30000-39999|10.1.1.18||proxy18 frps"
"tcp|4000-4050|10.1.1.18||ss1 tcp"
"udp|4000-4050|10.1.1.18||ss1 udp"
"tcp|4100-4150|10.1.1.18||ss2 tcp"
