
subnet=10.1.1.0/24
bridge=br-lan
wan=pppoe-out1
dynWanTag=dynWanDNAT
hairpinTag=HairpinNAT



# format
# "protocol|dstPort|toAddresses|toPorts|comment"
# toPorts可以省略，这时候会取dstPort的值
# "tcp|26| 10.1.1.22|  22 |mjr ssh"

