
subnet=10.1.1.0/24
bridge=br-lan
wan=pppoe-out1
dynWanTag=dynWanDNAT
hairpinTag=HairpinNAT



# format
# "protocol|dstPort|toAddresses|toPorts|comment"
# toPorts可以省略，这时候会取dstPort的值
# "tcp|26| 10.1.1.22|  22 |mjr ssh"

mappings=(
"tcp|8090|10.1.1.18|443|nginx reverse proxy dynWanDNAT"
"tcp|2018|10.1.1.18|22|proxy18 ssh dynWanDNAT"
"tcp|30000-39999|10.1.1.18||proxy18 frps dynWanDNAT"
"udp|30000-39999|10.1.1.18||proxy18 frps dynWanDNAT"
"tcp|4000-4050|10.1.1.18||ss1 tcp dynWanDNAT"
"udp|4000-4050|10.1.1.18||ss1 udp dynWanDNAT"
"tcp|4100-4150|10.1.1.18||ss2 tcp dynWanDNAT"
"udp|4100-4150|10.1.1.18||ss2 udp dynWanDNAT"
"udp|500,4500,1701|10.1.1.18||ikev2 vpn server dynWanDNAT"

"tcp|2026|10.1.1.26|22|dev26 ssh"
"tcp|5001,9091|10.1.1.22||dsm web transmission dynWanDNAT"
"tcp|8006|10.1.1.100|8006|pve admin dynWanDNAT"
)

